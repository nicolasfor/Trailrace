package com.cam.trailrace.modelo;

import java.util.List;
import java.util.Map;

/**
 * Created by nsanc on 13/09/2016.
 */
public class Piloto {

    //-----------------------------------------------
    //Atributos
    //-----------------------------------------------

    private String nombre;

    private String correo;

    private String pais;

    private String edad;

    private String telefono;

    private List<Vehiculo> vehiculos;

    private Map<String,Sesion> sesiones;

    public Piloto() {
    }

    public Piloto(String nombre, String correo, String pais, String edad, String telefono, List<Vehiculo> vehiculos, Map<String,Sesion> sesiones) {
        this.nombre = nombre;
        this.correo = correo;
        this.pais = pais;
        this.edad = edad;
        this.telefono = telefono;
        this.vehiculos = vehiculos;
        this.sesiones = sesiones;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public String getCorreo() {
        return correo;
    }

    public void setCorreo(String correo) {
        this.correo = correo;
    }

    public String getPais() {
        return pais;
    }

    public void setPais(String pais) {
        this.pais = pais;
    }

    public String getEdad() {
        return edad;
    }

    public void setEdad(String edad) {
        this.edad = edad;
    }

    public String getTelefono() {
        return telefono;
    }

    public void setTelefono(String telefono) {
        this.telefono = telefono;
    }

    public List<Vehiculo> getVehiculos() {
        return vehiculos;
    }

    public void setVehiculos(List<Vehiculo> vehiculos) {
        this.vehiculos = vehiculos;
    }

    public Map<String,Sesion> getSesiones() {
        return sesiones;
    }

    public void setSesiones(Map<String,Sesion> sesiones) {
        this.sesiones = sesiones;
    }

}
