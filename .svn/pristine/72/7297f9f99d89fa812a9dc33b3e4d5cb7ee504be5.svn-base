package com.cam.trailrace.modelo;

import java.util.Date;
import java.util.List;

/**
 * Created by nsanc on 13/09/2016.
 */
public class Pista {


    /**
     * Representa el nombre de la pista
     */
    private String nombre;

    /**
     * Contiene los puntos de referencia de la pista
     */
    private List<PuntoReferencia> puntosDeReferencia;


    private List<Punto> puntos;

    /**
     * Constructor de la clase pista
     * No inicializa ningun atributo
     */
    public Pista(String nombre, List<PuntoReferencia>puntosDeReferencia, List<Punto>puntos) {

        this.nombre=nombre;
        this.puntosDeReferencia=puntosDeReferencia;
        this.puntos = puntos;
    }

    public List<Punto> getPuntos() {
        return puntos;
    }

    public void setPuntos(List<Punto> puntos) {
        this.puntos = puntos;
    }

    public Pista(){

    }
    /**
     * Constructor de la clase pista
     * Solo inicializa el id de la pista
     * @param id El identificador unico de la pista
     */
    public Pista(long id) {
    }


    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public List<PuntoReferencia> getPuntosDeReferencia() {
        return puntosDeReferencia;
    }

    public void setPuntosDeReferencia(List<PuntoReferencia> puntosDeReferencia) {
        this.puntosDeReferencia = puntosDeReferencia;
    }

}
